<?xml version="1.0" encoding="utf-8"?><?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="20dip" />
            <gradient android:angle="-90" android:endColor="@color/colorButtonWhiteClick" android:startColor="@color/colorButtonWhiteClick" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="2px" android:color="#d2d1d6" />
            <corners android:radius="30dip" />
            <gradient android:angle="-90" android:endColor="@color/white" android:startColor="@color/white" />
        </shape>
    </item>
</selector>